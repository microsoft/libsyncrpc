{
  "name": "libsyncrpc",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "libsyncrpc",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@suchipi/dtsmd": "^1.1.0",
    "ava": "^6.0.1",
    "tinybench": "^2.9.0"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "npm run readme && napi artifacts",
    "bench": "npm run build && node ./bench.mjs",
    "build": "npm run readme && napi build --platform --release",
    "build:debug": "npm run readme && napi build --platform",
    "prepublishOnly": "npm run readme && napi prepublish -t npm",
    "readme": "node ./gen-readme.mjs",
    "test": "npm run build:debug && ava",
    "universal": "npm run readme && napi universal",
    "version": "napi version"
  }
}
